import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);    
    int i=0;
    while (sc.hasNext())
    {
        i=i+1;
        String s=sc.nextLine();
        System.out.println(+i+" "+s);
    }   
    }
}
