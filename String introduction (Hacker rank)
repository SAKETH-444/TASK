import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        int a=A.length();
        int b=B.length();
        System.out.println(a+b);
        b=A.compareTo(B);
        if (b>0)
        {
            System.out.println("Yes");
        }
        else
        {
            System.out.println("No");
        }
        String x=A.substring(0,1).toUpperCase();    //str.substring(a,b) means from a, b items are taken and toUpperCase() method makes that selected items to upper case
        String y=B.substring(0,1).toUpperCase();
        System.out.println(x+A.substring(1)+" "+y+B.substring(1));//str.substring(n) methord removes the nth item from the string and returns it. 
        
    
    }
}



